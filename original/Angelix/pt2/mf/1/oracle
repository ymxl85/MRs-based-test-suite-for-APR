#!/bin/bash
set -euo pipefail
SCRIPT_DIR=$(cd "$(dirname "${BASH_SOURCE[0]}")" && pwd)
test-pt2 () {
    diff <(${ANGELIX_RUN:-angelix-eval} ./pt2 "$1") <(angelix-eval $SCRIPT_DIR/../golden/pt2 "$1")
}

case "$1" in
    1)
      test-pt2  " x;\"\"
  "
      ;;
    2)
      test-pt2  " x;\"}
  "
      ;;
    3)
      test-pt2  " 5;9 
  "
      ;;
    4)
      test-pt2  " 9;x 
  "
      ;;
    5)
      test-pt2  " {;\"\"
  "
      ;;
    6)
      test-pt2  " x;U\"
  "
      ;;
    7)
      test-pt2  " x;\"\"
ß "
      ;;
    8)
      test-pt2  " x;\"\"
 ß"
      ;;
    9)
      test-pt2  "{"
      ;;
    10)
      test-pt2  " xÄ\"\"
  "
      ;;
    11)
