#!/bin/bash
set -euo pipefail
SCRIPT_DIR=$(cd "$(dirname "${BASH_SOURCE[0]}")" && pwd)
test-pt2 () {
    diff <(${ANGELIX_RUN:-angelix-eval} ./pt2 "$1") <(angelix-eval $SCRIPT_DIR/../golden/pt2 "$1")
}

case "$1" in
    1)
      test-pt2  "[w;yv}0-pUrL?:a4YY"
      ;;
    2)
      test-pt2  "vg/ x:w5c"
      ;;
    3)
      test-pt2  "Mf>G1Nd!?OLt!,"
      ;;
    4)
      test-pt2  "~C2:"
      ;;
    5)
      test-pt2  "6d*H>dwlf\$6!A?]"
      ;;
    6)
      test-pt2  "-"
      ;;
    7)
      test-pt2  "EU.&VscgfY!2P%"
      ;;
    8)
      test-pt2  "9h^>0._"
      ;;
    9)
      test-pt2  "Wo{9w^7GLUJ>JfM"
      ;;
    10)
      test-pt2  "aZbGh c.e^|nxp%{ "
      ;;
    11)
      test-pt2  "a"
      ;;
    12)
      test-pt2  "V41)_C{3\$1pRdM+R"
      ;;
    13)
      test-pt2  "zpn!x"
      ;;
    14)
      test-pt2  "yOry/"
      ;;
    15)
      test-pt2  "LtYRZyHa}I5"
      ;;
    16)
      test-pt2  "H!N*aUH57kyNq2wQ}p"
      ;;
    17)
      test-pt2  "%acGrU) |!S>nYb1"
      ;;
    18)
      test-pt2  "aC&"
      ;;
    19)
      test-pt2  "Hw+xhfOsd*5E#, 6Sd]"
      ;;
    20)
      test-pt2  "9hGoZj,;*K -=v]y"
      ;;
    21)
      test-pt2  "GP3xDS\8/:}W9rA.z&^W"
      ;;
    22)
      test-pt2  "~}W=&i"
      ;;
    23)
      test-pt2  "|p"
      ;;
    24)
      test-pt2  "gX4\`G"
      ;;
    25)
      test-pt2  "6UykgE]\".&/"
      ;;
    26)
      test-pt2  "ezlA~pU4vo\"X36RO(%"
      ;;
    27)
      test-pt2  "zXx#H ~1YEriF;~}]!G"
      ;;
    28)
      test-pt2  "D N-Aa;H{yz:?k+7dT"
      ;;
    29)
      test-pt2  "]<"
      ;;
    30)
      test-pt2  "c%I/{\"LD?"
      ;;
    31)
      test-pt2  "Q,*(uqZPn\`f~5A"
      ;;
    32)
      test-pt2  "TqnM;^^3accM-/znhX~"
      ;;
    33)
      test-pt2  "^.6_<V5im.e]0tS"
      ;;
    34)
      test-pt2  "*?(Q +lM*h="
      ;;
    35)
      test-pt2  "#x1SFhq]X"
      ;;
    36)
      test-pt2  "sld"
      ;;
    37)
      test-pt2  ";RVL6C2.OTQ4e5xrAv"
      ;;
    38)
      test-pt2  "OE|S"
      ;;
    39)
      test-pt2  ",8Q(cb/tF9uiF/K{A"
      ;;
    40)
      test-pt2  "t<_Zl\"Z]Kj]&"
      ;;
    41)
      test-pt2  "yjlwCCL4ssR\x&}_N"
      ;;
    42)
      test-pt2  "+G7}4%Gk{yfn.kzA"
      ;;
    43)
      test-pt2  "f%}SFXm"
      ;;
    44)
      test-pt2  "\"^tx.&=.\`mF8nyH-/g%h"
      ;;
    45)
      test-pt2  "6NxkAroI?waa\v9lKv\]"
      ;;
    46)
      test-pt2  "2!z.I]"
      ;;
    47)
      test-pt2  "!X3)rI\"sa[3H!"
      ;;
    48)
      test-pt2  "*mxweMuZ)wH@>O"
      ;;
    49)
      test-pt2  ">py_/1/FyV_"
      ;;
    50)
      test-pt2  "5<)cd]9wp2."
      ;;
    51)
      test-pt2  "E\`_Zp\\"
      ;;
    52)
      test-pt2  "Hdu][3M"
      ;;
    53)
      test-pt2  "NF;Wrz\$5WT|\"@J\""
      ;;
    54)
      test-pt2  "-J&MN"
      ;;
    55)
      test-pt2  "e-s"
      ;;
    56)
      test-pt2  "c)j?&AQD)>"
      ;;
    57)
      test-pt2  "yg^KljAf]M]~KN;"
      ;;
    58)
      test-pt2  ">6y0534u\"_h*"
      ;;
    59)
      test-pt2  "BeA]OI[a\}"
      ;;
    60)
      test-pt2  "{"
      ;;
esac
