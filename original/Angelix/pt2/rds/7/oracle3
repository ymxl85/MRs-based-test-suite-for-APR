      ;;
    45)
      test-pt2  "U0ebZrQq; N|3_C%pf;"
      ;;
    46)
      test-pt2  "=xLMW"
      ;;
    47)
      test-pt2  "B"
      ;;
    48)
      test-pt2  "7UvO%^Fo"
