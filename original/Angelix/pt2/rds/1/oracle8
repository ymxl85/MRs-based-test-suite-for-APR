#!/bin/bash
set -euo pipefail
SCRIPT_DIR=$(cd "$(dirname "${BASH_SOURCE[0]}")" && pwd)
test-pt2 () {
    diff <(${ANGELIX_RUN:-angelix-eval} ./pt2 "$1") <(angelix-eval $SCRIPT_DIR/../golden/pt2 "$1")
}

case "$1" in
    1)
      test-pt2  "%((OzF4J(rI3E)#"
      ;;
    2)
      test-pt2  "s{\`)OgS!I~n3"
      ;;
    3)
      test-pt2  "aa3"
      ;;
    4)
      test-pt2  ":SHS\9S"
      ;;
    5)
      test-pt2  "20+i1"
      ;;
    6)
      test-pt2  "FKr%(H4lpClb"
      ;;
    7)
      test-pt2  "Z"
      ;;
    8)
      test-pt2  "VnqZ!/;Q5H@Ues\$VO"
      ;;
    9)
      test-pt2  ".j6C(e&:v\$8"
      ;;
    10)
      test-pt2  "I@T#fXz,)[*8#"
      ;;
    11)
      test-pt2  "."
      ;;
    12)
      test-pt2  "Gj9vbmV%_W<v40t(al_"
      ;;
    13)
      test-pt2  "@&T_~_Tp;QxP_@"
      ;;
    14)
      test-pt2  "<?!q"
      ;;
    15)
      test-pt2  "P=.J3F#P;/9L\`:"
      ;;
    16)
      test-pt2  "V,ZXbo:DA*C"
      ;;
    17)
      test-pt2  "}h1AjVx"
      ;;
    18)
      test-pt2  "pCC(P!ppl0B+[F"
      ;;
    19)
      test-pt2  "X}*"
      ;;
    20)
      test-pt2  ":.1sOMFz/u7_-]Yuh"
      ;;
    21)
      test-pt2  "w!?2+icXQ:%w"
      ;;
    22)
      test-pt2  "Hu"
      ;;
    23)
      test-pt2  "Af}S/B?M"
      ;;
    24)
      test-pt2  "YIdW/E;~<EMk6+A8sO;"
      ;;
    25)
      test-pt2  "yF"
      ;;
    26)
      test-pt2  "(W{M|%I9h|{s<FRb:uh"
      ;;
    27)
      test-pt2  "I}0TtT0-wj,,yB\"i"
      ;;
    28)
      test-pt2  "jFzj.TJH^?XDQxn@1zf"
      ;;
    29)
      test-pt2  "FKVwRL(-@-TI"
      ;;
    30)
      test-pt2  "C.vZV?3Ub5}^D6xV"
      ;;
    31)
      test-pt2  "~T*j_F7"
      ;;
    32)
      test-pt2  "Y_Wf]"
      ;;
    33)
      test-pt2  "w]#Ay!J]LW:gX(\$"
      ;;
    34)
      test-pt2  "r}"
      ;;
    35)
      test-pt2  ".?Oc+L?U%MBi"
      ;;
    36)
      test-pt2  "m{[nG{d99({<ykrYn"
      ;;
    37)
      test-pt2  "dZh:CExxG45(D!\$xp"
      ;;
    38)
      test-pt2  "0\"KB=x^\""
      ;;
    39)
      test-pt2  "UdqhD"
      ;;
    40)
      test-pt2  "#;W"
      ;;
    41)
      test-pt2  "][6x@"
      ;;
    42)
      test-pt2  ".Z=has?N#xZP6j"
      ;;
    43)
      test-pt2  "tP53/["
      ;;
    44)
      test-pt2  "IPY"
      ;;
    45)
      test-pt2  "%{976Cnb1Q"
      ;;
    46)
      test-pt2  "flJ!8zbH~a=p%~Z%]:"
      ;;
    47)
      test-pt2  "7zTBy(fdU"
      ;;
    48)
      test-pt2  "\=\(&b?QY"
      ;;
    49)
      test-pt2  "SPgB*W"
      ;;
    50)
      test-pt2  "({r,FM<~zM@Q\A{"
      ;;
    51)
      test-pt2  "r9@I]4)J*Z"
      ;;
    52)
      test-pt2  "h|\`m@:zcXGvR\`V}i"
      ;;
    53)
      test-pt2  "<"
      ;;
    54)
      test-pt2  "yo:a{?dA<"
      ;;
    55)
      test-pt2  "WOGw%SS(>i*JQ"
      ;;
    56)
      test-pt2  "d\"#8&j1 \`ufIS"
      ;;
esac
