#!/bin/bash
set -euo pipefail
SCRIPT_DIR=$(cd "$(dirname "${BASH_SOURCE[0]}")" && pwd)
test-pt2 () {
    diff <(${ANGELIX_RUN:-angelix-eval} ./pt2 "$1") <(angelix-eval $SCRIPT_DIR/../golden/pt2 "$1")
}

case "$1" in
    1)
      test-pt2  "sfD\$b)Hq!y"
      ;;
    2)
      test-pt2  "N>YhydX1q]4qFYk9)_"
      ;;
    3)
      test-pt2  "mi1aS}q8\l["
      ;;
    4)
      test-pt2  "lM"
      ;;
    5)
      test-pt2  "Yob"
      ;;
    6)
      test-pt2  "#3:Ey!"
      ;;
    7)
      test-pt2  "Zl1_"
      ;;
    8)
      test-pt2  "/I&"
      ;;
    9)
      test-pt2  "3"
      ;;
    10)
      test-pt2  "655AV)OO1!f[[-O"
      ;;
    11)
      test-pt2  "@"
      ;;
    12)
      test-pt2  "]YZ,Cx_;X5LuKg,L?C.\""
      ;;
    13)
      test-pt2  "XI?2g#QV0\$Z&\`/3(Dd"
      ;;
    14)
      test-pt2  "louzLt#LB*FFD)HGB[e"
      ;;
    15)
      test-pt2  "uns)^;ZA^ia<7"
      ;;
    16)
      test-pt2  "lbv#aDSV6\$"
      ;;
    17)
      test-pt2  "py{!"
      ;;
    18)
      test-pt2  "-7%S(i-5\"T@"
      ;;
    19)
      test-pt2  "MV\$%y.>{w=3#iL51"
      ;;
    20)
      test-pt2  "BvDpqpi:gpyFD!/U:Mg"
      ;;
    21)
      test-pt2  "O^AdCLRadLFJ=:zq5"
      ;;
    22)
      test-pt2  "xDJFe"
      ;;
    23)
      test-pt2  "\%7].{=))J "
      ;;
    24)
      test-pt2  "ikGH,57>frL}Lx"
      ;;
    25)
      test-pt2  "x5K?\$%|Z "
      ;;
    26)
      test-pt2  "0=+D#"
      ;;
    27)
      test-pt2  "\SEimi"
      ;;
    28)
      test-pt2  "ynnx%oF"
      ;;
    29)
      test-pt2  "d9Zrs6KWrG>^L49e:)J\\"
      ;;
    30)
      test-pt2  "7(D=1b5 ("
      ;;
    31)
      test-pt2  "1O<;/+BW2l,<"
      ;;
    32)
      test-pt2  "N0~GksZ(RcUtX dL3}X"
      ;;
    33)
      test-pt2  "kT2tk~/ZXAXT<&ha"
      ;;
    34)
      test-pt2  "0\$3}-}5EH|&2/?m8"
      ;;
    35)
      test-pt2  "FeY"
      ;;
    36)
      test-pt2  "E"
      ;;
    37)
      test-pt2  "S2)oAX*"
      ;;
    38)
      test-pt2  ".1A"
      ;;
    39)
      test-pt2  "og"
      ;;
    40)
      test-pt2  "S\1\$i(XOQuC"
      ;;
    41)
      test-pt2  "7|)IGgOead{P%"
      ;;
    42)
      test-pt2  "9|)LO<"
      ;;
    43)
      test-pt2  "]OoWgVsy;"
      ;;
    44)
      test-pt2  "h=<2Oj@"
      ;;
    45)
      test-pt2  "NH1Z1CN\$Nwuf5P,Cn9"
      ;;
    46)
      test-pt2  "Xh(3"
      ;;
    47)
      test-pt2  "bM\;X(hA?#P_c|@=)["
      ;;
    48)
      test-pt2  "!Un"
      ;;
    49)
      test-pt2  "mL[gdQgQ9]3]x)"
      ;;
    50)
      test-pt2  "RM"
      ;;
    51)
      test-pt2  "2:UvhEI2,o"
      ;;
    52)
      test-pt2  "S2pha"
      ;;
    53)
      test-pt2  ")sO\$f)3WIh{mo)\"M=rCL"
      ;;
    54)
      test-pt2  "wYOP-."
      ;;
    55)
      test-pt2  "-"
      ;;
    56)
      test-pt2  "@c"
      ;;
esac
