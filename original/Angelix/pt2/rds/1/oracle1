    42)
      test-pt2  "?hTkgd{g@r=\$W}TG>9"
      ;;
    43)
      test-pt2  "?[%B( &L@"
      ;;
    44)
      test-pt2  "0;L;p~N"
      ;;
    45)
      test-pt2  "TB3jwP|qo"
      ;;
    46)
      test-pt2  "Tr6?{3hr_GO>F-"
      ;;
    47)
      test-pt2  "@-2#k@H5c!|7tcCJ<j8"
