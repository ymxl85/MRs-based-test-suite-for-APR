#!/bin/bash
set -euo pipefail
SCRIPT_DIR=$(cd "$(dirname "${BASH_SOURCE[0]}")" && pwd)
test-pt2 () {
    diff <(${ANGELIX_RUN:-angelix-eval} ./pt2 "$1") <(angelix-eval $SCRIPT_DIR/../golden/pt2 "$1")
}

case "$1" in
    1)
      test-pt2  "sah\`00rf:@V+"
      ;;
    2)
      test-pt2  "Yv=f31UP:ZGQUIM68("
      ;;
    3)
      test-pt2  "Bc "
      ;;
    4)
      test-pt2  "\$f2]q:"
      ;;
    5)
      test-pt2  "9{Xct5pCobW:U/c-"
      ;;
    6)
      test-pt2  "@*L<O:z\`CY_l=%Qrx8yc"
      ;;
    7)
      test-pt2  "#Rx^>xT?bj7B@N"
      ;;
    8)
      test-pt2  "};,rFf[h[n%mUowhO#"
      ;;
    9)
      test-pt2  "3d7RQX><7\`~EKQ5C\"u"
      ;;
    10)
      test-pt2  "=w"
      ;;
    11)
      test-pt2  "9JI3~8["
      ;;
    12)
      test-pt2  "\]bk25zaT,(hX~C?d"
      ;;
    13)
      test-pt2  "Y"
      ;;
    14)
      test-pt2  "&Sj}b5uH"
      ;;
    15)
      test-pt2  "136z!MBZ"
      ;;
    16)
      test-pt2  "7Qt@ 6^\`@u[Q#"
      ;;
    17)
      test-pt2  "oNavS1 6;,f"
      ;;
    18)
      test-pt2  "L-vg^kjW/"
      ;;
    19)
      test-pt2  "rK"
      ;;
    20)
      test-pt2  "k~nQ\`I3u9x"
      ;;
    21)
      test-pt2  "N;Mc->IMlS{v<y9Qri"
      ;;
    22)
      test-pt2  "dap(PIZ#[z4XX*>=)"
      ;;
    23)
      test-pt2  "znVK5#"
      ;;
    24)
      test-pt2  "fu|oS9eWO&An*NZg<!5"
      ;;
    25)
      test-pt2  "WoK*acSyuk_2m"
      ;;
    26)
      test-pt2  "(-Md73PFR)a.YI"
      ;;
    27)
      test-pt2  "A\"SL140YN"
      ;;
    28)
      test-pt2  "zl@ W8L^Z*"
      ;;
