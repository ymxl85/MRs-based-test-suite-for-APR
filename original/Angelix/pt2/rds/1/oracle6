      ;;
    30)
      test-pt2  "iC>\`\"LYEW /:E>[.>y"
      ;;
    31)
      test-pt2  " dr Y>YMx"
      ;;
    32)
      test-pt2  "p^d^|;DuJN}M|2kkgX}S"
      ;;
    33)
      test-pt2  "\$2pl]_rRe("
      ;;
    34)
      test-pt2  "-|a8"
      ;;
    35)
      test-pt2  "y<FBwvj9pa"
      ;;
    36)
      test-pt2  "EYNjDuy2T9~P)N-atF-i"
      ;;
    37)
      test-pt2  "PL\"i%Rh2-pW/Bem=x"
      ;;
    38)
      test-pt2  "7zW4~_gFx0An^Y9n1"
      ;;
    39)
      test-pt2  "ys=5~{[,V\$^?#;d#7^"
