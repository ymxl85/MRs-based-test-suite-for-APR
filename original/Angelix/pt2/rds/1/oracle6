      test-pt2  "eRkB:&G1|HuK.XTR("
      ;;
    43)
      test-pt2  ":=hEc,XQ6C-;=<@Y/e9"
      ;;
    44)
      test-pt2  "X#A\"gn.gK_*aL%b31"
      ;;
    45)
      test-pt2  "ah?eo"
      ;;
    46)
      test-pt2  "a\"z#WNCG\yb\$\$1?:I&"
