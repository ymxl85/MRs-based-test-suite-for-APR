#!/bin/bash
set -euo pipefail
SCRIPT_DIR=$(cd "$(dirname "${BASH_SOURCE[0]}")" && pwd)
test-pt2 () {
    diff <(${ANGELIX_RUN:-angelix-eval} ./pt2 "$1") <(angelix-eval $SCRIPT_DIR/../golden/pt2 "$1")
}

case "$1" in
    1)
      test-pt2  "k24\$iv4Tz]bQ"
      ;;
    2)
      test-pt2  "HE@4"
      ;;
    3)
      test-pt2  "#l\%<zx5U((F]hxPeqXC"
      ;;
    4)
      test-pt2  ">}PGI"
      ;;
    5)
      test-pt2  "\(l\`V,S\",,PC"
      ;;
    6)
      test-pt2  "]lXM9(M-}<tsbPMr-"
      ;;
    7)
      test-pt2  "k8w\41fq*nyTL^h7@"
      ;;
    8)
      test-pt2  "k9i9;QZ0?)\`z#K3e"
      ;;
    9)
      test-pt2  "{"
      ;;
    10)
      test-pt2  "n4v6_@~]%W&nC#yI"
      ;;
    11)
      test-pt2  "deX7"
      ;;
    12)
      test-pt2  "of651K"
      ;;
    13)
      test-pt2  "ogh{"
      ;;
    14)
      test-pt2  "3N)S\$Y"
      ;;
    15)
      test-pt2  "Q,y{zrEW+F"
      ;;
    16)
      test-pt2  "Z#"
      ;;
    17)
      test-pt2  ":Y,Rx=^B9\\"
      ;;
    18)
      test-pt2  "TafmMYKb[XRZ"
      ;;
    19)
      test-pt2  "YLlcZ%duu_c"
      ;;
    20)
      test-pt2  "ODneUl?ptC.+b)w"
      ;;
    21)
      test-pt2  ")akHUS?8dW"
      ;;
    22)
      test-pt2  "4@[c:J"
      ;;
    23)
      test-pt2  "|gcZ7d;A7n\$Vqh"
      ;;
    24)
      test-pt2  "?zH"
      ;;
    25)
      test-pt2  "}tS;ZnRNJ2BJ8"
      ;;
    26)
      test-pt2  "#Op5R"
      ;;
    27)
      test-pt2  "!k_v.|"
      ;;
    28)
      test-pt2  "K/4._JxS"
      ;;
    29)
      test-pt2  "Q.A"
      ;;
    30)
      test-pt2  ",ARd4&mQ/Nv\u=+qw_y"
      ;;
    31)
      test-pt2  ">xxp,8EmPw[#\zhw~x"
      ;;
    32)
      test-pt2  "L5{4Qnb 9o~%D6UQKT)F"
      ;;
    33)
      test-pt2  "?"
      ;;
    34)
      test-pt2  ">CIjH\"\"m@S1z>M2Z)sXt"
      ;;
    35)
      test-pt2  "jk&%WAMP3a+rE5"
      ;;
    36)
      test-pt2  ">K}H25)Tn"
      ;;
    37)
      test-pt2  "q&yU!{_Z|<fj"
      ;;
    38)
      test-pt2  "bs!p"
      ;;
    39)
      test-pt2  "Tr%ce-}((ZO"
      ;;
    40)
      test-pt2  "AT%!:]*4dsa~kqb3~gF%"
      ;;
    41)
      test-pt2  " ^iGyxa>e."
      ;;
    42)
      test-pt2  ")avO!*m;P bcMO2>E"
      ;;
    43)
      test-pt2  "jnrH:"
      ;;
    44)
      test-pt2  "Q/ IrX[CS|],8E+_"
      ;;
    45)
      test-pt2  "\$b/I:su=7O~aS<C5y?"
      ;;
    46)
      test-pt2  "|\`y;YHBn<"
      ;;
    47)
      test-pt2  "C6M-_\MsK=u,"
      ;;
    48)
      test-pt2  "{\$4CCJr"
      ;;
    49)
      test-pt2  "t)z<SQ2FDp>8#"
      ;;
    50)
      test-pt2  "m{!>_;\"vQ=y\`"
      ;;
    51)
      test-pt2  "K]\$Jv."
      ;;
    52)
      test-pt2  "QJ1!\fwGvL|e"
      ;;
    53)
      test-pt2  "os@ ew."
      ;;
    54)
      test-pt2  ">p\`FwFa9d"
      ;;
    55)
      test-pt2  "T}A).G"
      ;;
    56)
      test-pt2  "wf^tv7"
      ;;
