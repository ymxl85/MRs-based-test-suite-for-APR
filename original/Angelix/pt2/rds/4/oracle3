#!/bin/bash
set -euo pipefail
SCRIPT_DIR=$(cd "$(dirname "${BASH_SOURCE[0]}")" && pwd)
test-pt2 () {
    diff <(${ANGELIX_RUN:-angelix-eval} ./pt2 "$1") <(angelix-eval $SCRIPT_DIR/../golden/pt2 "$1")
}

case "$1" in
    1)
      test-pt2  "ULtjXhD\$91t"
      ;;
    2)
      test-pt2  "(~nq;(:&oM>,"
      ;;
    3)
      test-pt2  "]95%SB5/Rk"
      ;;
    4)
      test-pt2  "335!U4I#}h_;Y=luB["
      ;;
    5)
      test-pt2  "S!iATM/\"?!v<"
      ;;
    6)
      test-pt2  "o;/"
      ;;
    7)
      test-pt2  "S"
      ;;
    8)
      test-pt2  "!k3w!X;Y@2\$D{bnAM[Pa"
      ;;
    9)
      test-pt2  "CJALG/j\"hUC"
      ;;
    10)
      test-pt2  "/7U@%5g*D.#\\"
      ;;
    11)
      test-pt2  "V\"phRZPxljd!,1!@"
      ;;
    12)
      test-pt2  "%m1[sMO,?P|[Bpy-[b_)"
      ;;
    13)
      test-pt2  ";J\`{e*Ae-%]*i%5&[("
      ;;
    14)
      test-pt2  ";Qv4vyJGLZX\rV}9/"
      ;;
    15)
      test-pt2  "\$*.x"
      ;;
    16)
      test-pt2  "2!\$mrUc;7S5?mcS9"
      ;;
    17)
      test-pt2  "s\"26\`nAD<!MUkvyH"
      ;;
    18)
      test-pt2  "7cc8p\"?\`i9gjLZ4,c/"
      ;;
    19)
      test-pt2  "WT"
      ;;
    20)
      test-pt2  "Ab?U8,"
      ;;
    21)
      test-pt2  "ZQw{R+m"
      ;;
    22)
      test-pt2  "@D6Ta}=."
      ;;
    23)
      test-pt2  ":OA !%%T"
      ;;
    24)
      test-pt2  "~\"az"
      ;;
    25)
      test-pt2  "0KYxp?=>x3fuY"
      ;;
    26)
      test-pt2  "?j*\C9=!Gpz4}Id\\*M\$"
      ;;
    27)
      test-pt2  "|I-q;mi(%Pf"
      ;;
    28)
      test-pt2  "Wr\$!w_4c;V4Rj<tKqUE"
      ;;
    29)
      test-pt2  "apt=4M@t\$"
      ;;
    30)
      test-pt2  "]3A{\{T%O"
      ;;
    31)
      test-pt2  "\`8\"T^pmK4#XhD"
      ;;
    32)
      test-pt2  "1EQk9V #:fnI?D]RTDk"
      ;;
    33)
      test-pt2  "f+pWC82zJCp|QDRybIM"
      ;;
    34)
      test-pt2  "bu4Wc4cGq\`m:\"C+hTVF&"
      ;;
    35)
      test-pt2  "%xfR0*A"
      ;;
    36)
      test-pt2  "/4mCfG3\"\$Rhlk]*"
      ;;
    37)
      test-pt2  "{5~%<sO.ls<IZ5"
      ;;
    38)
      test-pt2  "pe9jVyW"
      ;;
    39)
      test-pt2  "T9\`c;/gT_"
      ;;
    40)
      test-pt2  ">"
      ;;
    41)
      test-pt2  "f}P8B7V)c)i2yHB"
      ;;
    42)
      test-pt2  "jWYLz2"
      ;;
    43)
      test-pt2  "AGlS3?~SkAT"
      ;;
    44)
      test-pt2  "8bDgK-LhtX62#V"
      ;;
    45)
      test-pt2  "Xc6Vp; AZ+~Vt5^N-\0"
      ;;
    46)
      test-pt2  "s2rG_,|.tFtc"
      ;;
    47)
      test-pt2  "iMg(Co?7 6,/CZSC3"
      ;;
    48)
      test-pt2  "djr<2eSu\"mKyFI@}"
      ;;
    49)
      test-pt2  "!r"
      ;;
    50)
      test-pt2  "-%~:pRye<_# ?AhNV/1w"
      ;;
esac
