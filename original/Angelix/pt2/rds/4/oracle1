#!/bin/bash
set -euo pipefail
SCRIPT_DIR=$(cd "$(dirname "${BASH_SOURCE[0]}")" && pwd)
test-pt2 () {
    diff <(${ANGELIX_RUN:-angelix-eval} ./pt2 "$1") <(angelix-eval $SCRIPT_DIR/../golden/pt2 "$1")
}

case "$1" in
    1)
      test-pt2  "? \`ifYrlFh{QmpYNo=j5"
      ;;
    2)
      test-pt2  "%m0WramGRS"
      ;;
    3)
      test-pt2  "j,NP,lU8 l1y\$6=ni"
      ;;
    4)
      test-pt2  "/n)PTatiw{D&!z&"
      ;;
    5)
      test-pt2  "9@X&@VyC[Y"
      ;;
    6)
      test-pt2  "Tx|Kq+"
      ;;
    7)
      test-pt2  "CZ"
      ;;
    8)
      test-pt2  "C4\"J+(}d\"8[c"
      ;;
    9)
      test-pt2  "]9A|A2_V8_+{r;:q"
      ;;
    10)
      test-pt2  "#TS]w6r>RdeTR\$XX</"
      ;;
    11)
      test-pt2  "V#o0&h?5g^d"
      ;;
    12)
      test-pt2  "?ZaJ,oq5pbB[C%%F "
      ;;
    13)
      test-pt2  "pVCmu@kqy,q3_hP_"
      ;;
    14)
      test-pt2  "\$"
      ;;
    15)
      test-pt2  "cdF"
      ;;
    16)
      test-pt2  "Pf:+8kuDz\"WGn^><9I"
      ;;
    17)
      test-pt2  "X"
      ;;
    18)
      test-pt2  "+[I1+ -Mj"
      ;;
    19)
      test-pt2  "\`\$b"
      ;;
    20)
      test-pt2  ",QZ(#f"
      ;;
    21)
      test-pt2  "+:2;D?y&O,puH:VYw\\"
      ;;
    22)
      test-pt2  "A#3U,N+ddtm6=&M*;"
      ;;
    23)
      test-pt2  "hx-lD=!Kqe.<@9b"
      ;;
    24)
      test-pt2  "6CW55o)}l\`21_kh;i)8|"
      ;;
    25)
      test-pt2  "GBo"
      ;;
    26)
      test-pt2  ";{"
      ;;
    27)
      test-pt2  "jOW_ d=uQ-4}Oa*d"
      ;;
    28)
      test-pt2  "ec+VIhPWjLg)"
      ;;
    29)
      test-pt2  "s"
      ;;
    30)
      test-pt2  "I"
      ;;
    31)
      test-pt2  "Z|}7{rWhyo5rcH1"
      ;;
    32)
      test-pt2  "L]JbUE"
      ;;
    33)
      test-pt2  "eV%P"
      ;;
    34)
      test-pt2  "Y~JlW&gJTJFcHn"
      ;;
    35)
      test-pt2  "(]?TLO}2,@"
      ;;
    36)
      test-pt2  ")wyp#eP\$?Mmu,"
      ;;
    37)
      test-pt2  "lhC"
      ;;
    38)
      test-pt2  "U]\`Qm=@4IT|e7)L<% md"
      ;;
    39)
      test-pt2  "pHFaS\\\$R0oj"
      ;;
    40)
      test-pt2  "_"
      ;;
    41)
      test-pt2  "G)sURW*+!~qM"
      ;;
    42)
      test-pt2  "e;?k5L^9>?pU~?r&|"
      ;;
    43)
      test-pt2  "PANl#q.AcF M(N\$#tj"
      ;;
    44)
      test-pt2  "MF"
      ;;
    45)
      test-pt2  "}oHj+O"
      ;;
    46)
      test-pt2  "}Hl=:f,sc~x\`NR"
      ;;
    47)
      test-pt2  "7t.:<P_lR"
      ;;
    48)
      test-pt2  "!</K1-j\"Zupt),c&7b"
      ;;
    49)
      test-pt2  "yT%i"
      ;;
    50)
      test-pt2  "{KM<b8ypBZ"
      ;;
esac
