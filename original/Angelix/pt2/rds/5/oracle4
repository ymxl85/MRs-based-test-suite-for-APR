#!/bin/bash
set -euo pipefail
SCRIPT_DIR=$(cd "$(dirname "${BASH_SOURCE[0]}")" && pwd)
test-pt2 () {
    diff <(${ANGELIX_RUN:-angelix-eval} ./pt2 "$1") <(angelix-eval $SCRIPT_DIR/../golden/pt2 "$1")
}

case "$1" in
    1)
      test-pt2  "Q,z"
      ;;
    2)
      test-pt2  "\\"
      ;;
    3)
      test-pt2  "X]/Pm\`"
      ;;
    4)
      test-pt2  "pAY.e.y=mM62)7U"
      ;;
    5)
      test-pt2  ":=z(m\$"
      ;;
    6)
      test-pt2  "m#Y*CdosAh"
      ;;
    7)
      test-pt2  "jOZzg{Hm7%\"7-A%vQVw"
      ;;
    8)
      test-pt2  "IIMLVzjq@]OgkX}"
      ;;
    9)
      test-pt2  "F\"_.lcI|0jb1w=t\"N.\""
      ;;
    10)
      test-pt2  "W\$HCJA1!jm{Pp3y"
      ;;
    11)
      test-pt2  "7]poLm"
      ;;
    12)
      test-pt2  "0dZl%?\"X7"
      ;;
    13)
      test-pt2  "Ku0ctm1n>6E]4OM;qR-"
      ;;
    14)
      test-pt2  "uzo@HpF"
      ;;
    15)
      test-pt2  "bnRU8!g!"
      ;;
    16)
      test-pt2  "~?CkZui eq2R["
      ;;
    17)
      test-pt2  "n-<Qc/xU\nP7p"
      ;;
    18)
      test-pt2  "=j,9|g%Gm\^>h(#0"
      ;;
    19)
      test-pt2  "0Voz\`1qVd!"
      ;;
    20)
      test-pt2  "=! mT}D]pka.OC"
      ;;
    21)
      test-pt2  "I79@t-ZH<:"
      ;;
    22)
      test-pt2  "YN,j]%]LkvJ4cZ) S2"
      ;;
    23)
      test-pt2  "2f5J*pzJ*_}"
      ;;
    24)
      test-pt2  "vk"
      ;;
    25)
      test-pt2  "-UVJxNM6Xfp<="
      ;;
    26)
      test-pt2  "scTn!EP@]"
      ;;
    27)
      test-pt2  "sKBlIM+B}D\`I}"
      ;;
    28)
      test-pt2  "\"DdonR58^ta?\$\$)FL"
      ;;
    29)
      test-pt2  "!r{"
      ;;
    30)
      test-pt2  "#gH9piq?dqYX.["
      ;;
    31)
      test-pt2  "Xx\\,_{~G-?;!*r"
      ;;
    32)
      test-pt2  "Q=D0"
      ;;
    33)
      test-pt2  "mNL\$*T21R)\/FmTR%<"
      ;;
    34)
      test-pt2  "xw"
      ;;
esac
