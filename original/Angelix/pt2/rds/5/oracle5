#!/bin/bash
set -euo pipefail
SCRIPT_DIR=$(cd "$(dirname "${BASH_SOURCE[0]}")" && pwd)
test-pt2 () {
    diff <(${ANGELIX_RUN:-angelix-eval} ./pt2 "$1") <(angelix-eval $SCRIPT_DIR/../golden/pt2 "$1")
}

case "$1" in
    1)
      test-pt2  "P\".F.j,ELK|?n\$p9SF"
      ;;
    2)
      test-pt2  "Q\"P"
      ;;
    3)
      test-pt2  "~\"qLny]\`IT\"~~p["
      ;;
    4)
      test-pt2  "L2E*FIEX_w^~r&7nj\`"
      ;;
    5)
      test-pt2  "()S>#\"knw~qMj2NyY"
      ;;
    6)
      test-pt2  "Pl\N"
      ;;
    7)
      test-pt2  "4p3uS18%!9\`|.6%cm"
      ;;
    8)
      test-pt2  "aB W&C "
      ;;
    9)
      test-pt2  "^KX"
      ;;
    10)
      test-pt2  "93o<e2[P9mtxg\+"
      ;;
    11)
      test-pt2  "_iCpeGEs, 1uLmgoilk"
      ;;
    12)
      test-pt2  "YxFF=)p"
      ;;
    13)
      test-pt2  "0^>"
      ;;
    14)
      test-pt2  "Sz3B9#Y>F#m~/]AAB&"
      ;;
    15)
      test-pt2  "7mSm=JpxY^R*}!Isr\$@"
      ;;
    16)
      test-pt2  "uB@yz@v#Q&nE ,"
      ;;
    17)
      test-pt2  "m=bLokmm#F:b<A^"
      ;;
    18)
      test-pt2  "r8Yr33va\h>*3{b_aC"
      ;;
    19)
      test-pt2  "*crc;>L>"
      ;;
    20)
      test-pt2  "8^wg w539l"
      ;;
    21)
      test-pt2  "~YVfol%.0/-=o=lMw"
      ;;
    22)
      test-pt2  "b/BM\w2BlAwj"
      ;;
    23)
      test-pt2  "OL!qB}"
      ;;
    24)
      test-pt2  "GM[{F"
      ;;
    25)
      test-pt2  "o:hMdx67>"
      ;;
    26)
      test-pt2  "[eK<a&}ml6m"
      ;;
    27)
      test-pt2  ".\"%~gxV_z"
      ;;
    28)
      test-pt2  "h"
      ;;
    29)
      test-pt2  "Vf:S}(},3w3\"YEOVb"
      ;;
    30)
      test-pt2  "<qb"
      ;;
    31)
      test-pt2  "rE3j<fb.ifKz>^,"
      ;;
    32)
      test-pt2  "s4LZzf)\"uu-kT9"
      ;;
    33)
      test-pt2  "R{F.SQ\`c3Js7UfS^Ez{^"
      ;;
    34)
      test-pt2  "M*k?j a%{0"
      ;;
esac
