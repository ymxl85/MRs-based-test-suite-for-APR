#!/bin/bash
set -euo pipefail
SCRIPT_DIR=$(cd "$(dirname "${BASH_SOURCE[0]}")" && pwd)
test-tcas () {
    diff -q <(${ANGELIX_RUN:-eval} ./tcas $1) <($SCRIPT_DIR/../golden/tcas $1) > /dev/null
}

case "$1" in
    1)
      test-tcas  "1264 1 1 -8464 -10000 -911 0 -8192 4336 0 2 0"
      ;;
    2)
      test-tcas  "5272 1 1 3184 112 912 1 -9988 -1936 0 2 1"
      ;;
    3)
      test-tcas  "5272 1 1 3184 112 912 0 -9988 -1936 0 2 1"
      ;;
    4)
      test-tcas  "5272 1 1 3184 112 912 2 -9988 -1936 0 2 1"
      ;;
    5)
      test-tcas  "5272 1 1 3184 112 912 3 -9988 -1936 0 2 1"
      ;;
    6)
      test-tcas  "1264 1 1 -4687 -10000 -4687 0 -8192 4336 0 2 0"
      ;;
    7)
      test-tcas  "5272 1 1 2048 112 2048 1 -9988 -1936 0 2 1"
      ;;
    8)
      test-tcas  "5272 1 1 2048 112 2048 0 -9988 -1936 0 2 1"
      ;;
    9)
      test-tcas  "5272 1 1 2048 112 2048 2 -9988 -1936 0 2 1"
      ;;
    10)
      test-tcas  "5272 1 1 2048 112 2048 3 -9988 -1936 0 2 1"
      ;;
    11)
      test-tcas  "4048 1 1 6368 80 6368 0 -4096 5872 0 2 0"
      ;;
    12)
      test-tcas  "6264 1 1 -9999 240 -10000 0 -8192 2288 0 2 0"
      ;;
    13)
      test-tcas  "2064 1 1 6368 2064 6368 0 -4096 5872 0 2 0"
      ;;
    14)
      test-tcas  "6264 1 1 -9999 240 -9999 0 -8192 2288 0 2 0"
      ;;
    15)
      test-tcas  "6264 1 1 2272 240 6369 1 -8192 5872 0 2 0"
      ;;
    16)
      test-tcas  "9272 1 1 6961 240 240 1 2300 2408 0 2 1"
      ;;
    17)
      test-tcas  "2296 1 1 -9999 240 -10000 1 6032 9972 0 2 0"
      ;;
    18)
      test-tcas  "6264 1 1 4320 240 4320 1 -8192 5872 0 2 0"
      ;;
    19)
      test-tcas  "4756 1 1 6961 4756 240 1 2300 2408 0 2 1"
      ;;
    20)
      test-tcas  "1268 1 1 -9999 1268 -10000 1 6032 9972 0 2 0"
      ;;
    21)
      test-tcas  "6264 1 1 2272 240 6369 0 -8192 5872 0 2 0"
      ;;
    22)
      test-tcas  "6376 1 1 -1199 240 -1200 0 2300 2400 0 2 1"
      ;;
    23)
      test-tcas  "3096 1 1 2128 240 2288 0 -8192 4080 0 2 0"
      ;;
    24)
      test-tcas  "6264 1 1 4320 240 4320 0 -8192 5872 0 2 0"
      ;;
    25)
      test-tcas  "3308 1 1 -1199 3308 -1200 0 2300 2400 0 2 1"
      ;;
    26)
      test-tcas  "3096 1 1 2208 240 2208 0 -8192 4080 0 2 0"
      ;;
    27)
      test-tcas  "6264 1 1 2272 240 6369 2 -8192 5872 0 2 0"
      ;;
    28)
      test-tcas  "3192 1 1 -3087 240 -7440 2 2300 2400 0 2 1"
      ;;
    29)
      test-tcas  "3192 1 1 2272 80 2288 2 -9728 -8992 0 2 0"
      ;;
    30)
      test-tcas  "6264 1 1 4320 240 4320 2 -8192 5872 0 2 0"
      ;;
    31)
      test-tcas  "1716 1 1 -3087 1716 -7440 2 2300 2400 0 2 1"
      ;;
    32)
      test-tcas  "3192 1 1 2280 80 2280 2 -9728 -8992 0 2 0"
      ;;
    33)
      test-tcas  "6264 1 1 2272 240 6369 3 -8192 5872 0 2 0"
      ;;
    34)
      test-tcas  "5240 1 1 8401 240 240 3 2300 2408 0 2 1"
      ;;
    35)
      test-tcas  "2296 1 1 -9999 80 -10000 3 5888 8368 0 2 0"
      ;;
    36)
      test-tcas  "6264 1 1 4320 240 4320 3 -8192 5872 0 2 0"
      ;;
    37)
      test-tcas  "2740 1 1 8401 2740 240 3 2300 2408 0 2 1"
      ;;
    38)
      test-tcas  "1188 1 1 -9999 1188 -10000 3 5888 8368 0 2 0"
      ;;
esac
