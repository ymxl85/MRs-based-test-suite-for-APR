#!/bin/bash
set -euo pipefail
SCRIPT_DIR=$(cd "$(dirname "${BASH_SOURCE[0]}")" && pwd)
test-tcas () {
    diff -q <(${ANGELIX_RUN:-eval} ./tcas $1) <($SCRIPT_DIR/../golden/tcas $1) > /dev/null
}

case "$1" in
    1)
      test-tcas  "6264 1 1 -2016 -7344 -9008 0 400 500 0 2 1"
      ;;
    2)
      test-tcas  "3088 1 1 -5936 336 -1840 1 7956 500 0 2 1"
      ;;
    3)
