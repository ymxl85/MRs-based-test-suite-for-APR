#!/bin/bash
set -euo pipefail
SCRIPT_DIR=$(cd "$(dirname "${BASH_SOURCE[0]}")" && pwd)
test-tcas () {
    diff -q <(${ANGELIX_RUN:-eval} ./tcas $1) <($SCRIPT_DIR/../golden/tcas $1) > /dev/null
}

case "$1" in
    1)
      test-tcas  "-6384 0 0 -1552 -1296 2288 0 -6416 -6416 0 1 0"
      ;;
    2)
      test-tcas  "3184 1 1 2288 3696 -10000 0 -4 -6320 0 2 0"
      ;;
    3)
      test-tcas  "144 1 1 4336 1552 2288 3 -8292 -8192 0 2 1"
      ;;
    4)
      test-tcas  "-9136 1 1 -3856 -6416 -1808 0 0 0 0 2 0"
      ;;
    5)
      test-tcas  "208 1 1 4208 4848 6256 1 -176 -8464 0 2 0"
      ;;
    6)
      test-tcas  "144 1 1 4336 1552 2288 2 -8292 -8192 0 2 1"
      ;;
    7)
      test-tcas  "3576 1 1 6258 848 6224 3 10000 10000 0 2 0"
      ;;
    8)
      test-tcas  "-1872 1 1 -1936 16 -9040 0 2292 2396 0 2 1"
      ;;
    9)
      test-tcas  "272 1 1 6384 2736 8416 1 4081 752 0 2 0"
      ;;
    10)
      test-tcas  "272 1 1 240 2288 -8992 2 6256 6356 0 2 1"
      ;;
    11)
      test-tcas  "240 1 1 8428 848 8301 0 0 0 0 2 0"
      ;;
    12)
      test-tcas  "-10000 1 1 -9991 2288 -9989 0 -9064 -9999 0 2 1"
      ;;
    13)
      test-tcas  "-5136 1 1 -2000 112 -9104 3 2288 2290 0 2 0"
      ;;
    14)
      test-tcas  "3304 1 1 -9231 6064 -10000 3 -8128 5872 0 2 0"
      ;;
    15)
      test-tcas  "48 1 1 6352 7248 6320 1 -4160 5872 0 2 0"
      ;;
    16)
      test-tcas  "272 1 1 -3957 1264 -3959 1 2288 2290 0 2 0"
      ;;
    17)
      test-tcas  "1256 1 1 1040 240 3256 1 -3 -6416 0 2 0"
      ;;
    18)
      test-tcas  "-10000 1 1 -9991 2288 -9989 2 -9064 -9999 0 2 1"
      ;;
    19)
      test-tcas  "3304 1 1 -9231 6064 -10000 0 -8128 5872 0 2 0"
      ;;
    20)
      test-tcas  "760 1 1 -10000 16 -9999 1 6332 752 0 2 1"
      ;;
    21)
      test-tcas  "-9776 1 1 -9648 7760 2288 0 6316 2288 0 2 1"
      ;;
    22)
      test-tcas  "1256 1 1 1040 240 3256 0 -3 -6416 0 2 0"
      ;;
    23)
      test-tcas  "4344 1 1 4336 6160 -10000 0 4164 5360 0 2 1"
      ;;
    24)
      test-tcas  "6264 1 1 4336 2288 4337 0 8316 2800 0 2 1"
      ;;
    25)
      test-tcas  "-2896 1 1 -9616 3152 -9104 2 6316 2288 0 2 1"
      ;;
    26)
      test-tcas  "1256 1 1 1040 240 3256 2 -3 -6416 0 2 0"
      ;;
    27)
      test-tcas  "3304 1 1 -9231 6064 -10000 2 -8128 5872 0 2 0"
      ;;
    28)
      test-tcas  "6264 1 1 -1039 1776 -7968 2 7196 7392 0 2 0"
      ;;
    29)
      test-tcas  "-10000 1 1 -9991 2288 -9989 3 -9064 -9999 0 2 1"
      ;;
    30)
      test-tcas  "16 1 1 -10000 2128 -9999 3 6300 2288 0 2 1"
