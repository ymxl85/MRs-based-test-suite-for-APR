      test-tcas  "1896 1 1 2704 80 240 1 10000 10000 0 2 0"
      ;;
    25)
      test-tcas  "2296 1 1 -4016 16 -5904 3 10000 10000 0 2 0"
      ;;
    26)
      test-tcas  "2296 1 1 -7952 80 -10000 3 -8000 5872 0 2 0"
      ;;
    27)
      test-tcas  "752 1 1 -1584 -8176 6368 1 -4196 -6416 0 2 1"
      ;;
    28)
      test-tcas  "3280 1 1 -1712 -10000 4848 2 8212 8305 0 2 1"
      ;;
