#!/bin/bash
set -euo pipefail
SCRIPT_DIR=$(cd "$(dirname "${BASH_SOURCE[0]}")" && pwd)
test-tcas () {
    diff -q <(${ANGELIX_RUN:-eval} ./tcas $1) <($SCRIPT_DIR/../golden/tcas $1) > /dev/null
}

case "$1" in
    1)
      test-tcas  "578 0 221 380 733 9694 3 51 733 1 1 1"
      ;;
    2)
      test-tcas  "333 0 787 481 861 9714 0 734 9 0 0 0"
      ;;
    3)
      test-tcas  "214 0 187 533 516 9502 0 439 860 1 1 0"
      ;;
    4)
      test-tcas  "49 1 691 293 192 9242 1 572 294 0 1 0"
      ;;
    5)
      test-tcas  "819 0 261 800 469 8632 3 980 110 1 0 1"
      ;;
    6)
      test-tcas  "775 0 868 958 954 1101 3 748 893 1 0 1"
      ;;
    7)
      test-tcas  "491 0 436 741 466 3205 0 642 869 1 0 0"
      ;;
    8)
      test-tcas  "699 1 708 788 303 8525 3 234 514 1 0 0"
      ;;
    9)
      test-tcas  "975 0 998 985 892 7234 1 585 560 1 1 1"
      ;;
    10)
      test-tcas  "337 1 848 480 411 203 1 242 605 1 1 0"
      ;;
    11)
      test-tcas  "687 1 959 584 120 230 0 355 970 1 1 1"
      ;;
    12)
      test-tcas  "729 1 512 57 136 2582 3 785 705 1 0 0"
      ;;
    13)
      test-tcas  "535 1 34 463 174 2594 3 305 754 1 1 0"
      ;;
    14)
      test-tcas  "348 1 394 520 505 9564 2 255 257 1 1 1"
      ;;
    15)
      test-tcas  "411 0 241 747 860 3625 3 417 740 1 1 0"
      ;;
    16)
      test-tcas  "718 1 310 117 370 5630 1 344 627 0 1 1"
      ;;
    17)
      test-tcas  "501 0 455 20 870 9408 3 949 929 1 1 1"
      ;;
    18)
      test-tcas  "10 0 42 411 894 964 0 831 370 1 0 1"
      ;;
esac
