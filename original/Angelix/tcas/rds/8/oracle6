#!/bin/bash
set -euo pipefail
SCRIPT_DIR=$(cd "$(dirname "${BASH_SOURCE[0]}")" && pwd)
test-tcas () {
    diff -q <(${ANGELIX_RUN:-eval} ./tcas $1) <($SCRIPT_DIR/../golden/tcas $1) > /dev/null
}

case "$1" in
    1)
      test-tcas  "363 0 826 979 747 7886 3 349 909 1 0 1"
      ;;
    2)
      test-tcas  "112 0 869 599 811 7401 3 391 353 0 0 0"
      ;;
    3)
      test-tcas  "275 1 979 234 625 9067 1 563 646 0 0 0"
      ;;
    4)
      test-tcas  "990 1 952 981 337 101 0 576 927 1 0 1"
      ;;
    5)
      test-tcas  "71 1 971 372 109 3900 2 904 668 1 1 1"
      ;;
    6)
      test-tcas  "316 1 372 72 856 5270 1 348 593 1 0 0"
      ;;
    7)
      test-tcas  "866 1 661 398 16 677 3 117 771 0 1 0"
      ;;
    8)
      test-tcas  "675 0 723 892 838 2497 0 639 752 1 0 1"
      ;;
    9)
      test-tcas  "350 1 255 857 790 4831 0 677 668 0 1 0"
      ;;
    10)
      test-tcas  "526 0 605 348 490 1708 1 549 901 0 1 1"
      ;;
    11)
      test-tcas  "570 0 755 748 104 6409 0 553 374 1 0 1"
      ;;
    12)
      test-tcas  "774 1 760 579 412 3311 3 867 710 1 0 0"
      ;;
    13)
      test-tcas  "235 1 953 750 713 934 2 407 336 1 0 0"
      ;;
    14)
      test-tcas  "422 0 248 925 447 6879 0 270 249 1 1 1"
      ;;
    15)
      test-tcas  "954 1 275 180 784 2063 2 508 449 0 0 0"
      ;;
    16)
      test-tcas  "287 0 596 526 621 2996 2 84 115 0 0 0"
      ;;
    17)
      test-tcas  "4 0 28 311 214 6927 0 891 299 0 1 0"
      ;;
    18)
      test-tcas  "32 1 101 845 280 5449 2 783 461 0 1 0"
      ;;
esac
