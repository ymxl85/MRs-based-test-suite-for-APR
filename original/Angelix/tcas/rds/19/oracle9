#!/bin/bash
set -euo pipefail
SCRIPT_DIR=$(cd "$(dirname "${BASH_SOURCE[0]}")" && pwd)
test-tcas () {
    diff -q <(${ANGELIX_RUN:-eval} ./tcas $1) <($SCRIPT_DIR/../golden/tcas $1) > /dev/null
}

case "$1" in
    1)
      test-tcas  "999 1 830 569 161 2470 1 875 780 0 0 0"
      ;;
    2)
      test-tcas  "640 1 695 508 218 2672 2 54 721 0 1 1"
      ;;
    3)
      test-tcas  "365 0 11 475 756 3573 1 225 810 1 0 0"
      ;;
    4)
      test-tcas  "129 0 68 128 855 8467 2 607 72 0 1 1"
      ;;
    5)
      test-tcas  "507 0 45 664 877 5224 1 245 490 0 0 1"
      ;;
    6)
      test-tcas  "837 1 142 524 770 3073 1 163 528 0 1 0"
      ;;
    7)
      test-tcas  "303 0 730 833 266 5466 2 106 973 0 0 0"
      ;;
    8)
      test-tcas  "840 0 104 410 964 8507 3 538 881 0 0 0"
      ;;
    9)
      test-tcas  "17 0 935 204 444 6063 2 108 305 1 1 1"
      ;;
    10)
      test-tcas  "668 0 956 608 899 5296 1 937 601 1 0 0"
      ;;
    11)
      test-tcas  "42 0 605 357 545 9845 0 849 713 1 0 1"
      ;;
    12)
      test-tcas  "514 0 462 458 189 2372 0 958 654 0 1 1"
      ;;
    13)
      test-tcas  "597 0 217 82 719 3580 0 19 659 1 0 1"
      ;;
    14)
      test-tcas  "964 1 694 251 119 1143 1 410 30 1 1 0"
      ;;
    15)
      test-tcas  "562 0 44 828 694 9285 1 779 607 0 0 0"
      ;;
    16)
      test-tcas  "558 0 690 418 690 7912 1 69 47 1 1 0"
      ;;
    17)
      test-tcas  "916 1 897 627 293 9422 3 971 695 0 1 0"
      ;;
    18)
      test-tcas  "769 1 990 455 143 1325 3 714 757 0 0 1"
      ;;
esac
