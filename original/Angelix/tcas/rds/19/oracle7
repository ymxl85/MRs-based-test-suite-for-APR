#!/bin/bash
set -euo pipefail
SCRIPT_DIR=$(cd "$(dirname "${BASH_SOURCE[0]}")" && pwd)
test-tcas () {
    diff -q <(${ANGELIX_RUN:-eval} ./tcas $1) <($SCRIPT_DIR/../golden/tcas $1) > /dev/null
}

case "$1" in
    1)
      test-tcas  "583 1 22 625 255 4956 0 502 474 1 1 1"
      ;;
    2)
      test-tcas  "683 1 952 180 436 9883 3 959 757 0 1 1"
      ;;
    3)
      test-tcas  "155 0 1 37 670 9445 2 316 175 0 1 0"
      ;;
    4)
      test-tcas  "562 1 839 935 380 6358 0 463 947 0 0 0"
      ;;
    5)
      test-tcas  "949 1 582 852 72 3592 0 41 255 0 0 0"
      ;;
    6)
      test-tcas  "764 0 890 820 370 3834 2 176 592 1 1 1"
      ;;
    7)
      test-tcas  "140 1 574 135 395 4069 1 531 10 1 0 1"
      ;;
    8)
      test-tcas  "511 1 180 588 40 6656 1 223 264 1 0 0"
      ;;
    9)
      test-tcas  "812 1 446 407 516 9203 2 518 155 1 0 0"
      ;;
    10)
      test-tcas  "371 0 12 541 903 9952 0 422 336 1 0 0"
      ;;
    11)
      test-tcas  "93 0 400 662 494 3186 1 328 329 0 0 0"
      ;;
    12)
      test-tcas  "534 0 940 897 236 451 3 786 674 1 0 1"
      ;;
    13)
      test-tcas  "190 1 138 734 820 8060 3 174 780 1 0 0"
      ;;
    14)
      test-tcas  "498 0 851 858 688 9507 3 649 409 0 1 1"
      ;;
    15)
      test-tcas  "919 1 612 800 648 2970 2 174 588 1 0 0"
      ;;
    16)
      test-tcas  "910 0 523 292 698 9090 1 668 257 1 0 0"
      ;;
    17)
      test-tcas  "588 0 303 15 250 2692 1 471 27 0 0 1"
      ;;
    18)
      test-tcas  "529 0 323 141 96 3957 0 222 135 0 0 1"
      ;;
esac
