#!/bin/bash
set -euo pipefail
SCRIPT_DIR=$(cd "$(dirname "${BASH_SOURCE[0]}")" && pwd)
test-tcas () {
    diff -q <(${ANGELIX_RUN:-eval} ./tcas $1) <($SCRIPT_DIR/../golden/tcas $1) > /dev/null
}

case "$1" in
    1)
      test-tcas  "67 1 541 729 128 2098 1 820 962 0 1 0"
      ;;
    2)
      test-tcas  "205 1 411 413 252 7734 1 983 710 0 1 1"
      ;;
    3)
      test-tcas  "829 1 10 224 110 6641 1 65 142 0 1 1"
      ;;
    4)
      test-tcas  "19 1 908 123 321 2706 0 989 785 0 1 0"
      ;;
    5)
      test-tcas  "211 0 841 320 111 982 3 971 868 0 1 0"
      ;;
    6)
      test-tcas  "882 1 679 654 327 3316 1 518 623 1 1 0"
      ;;
    7)
      test-tcas  "674 1 908 343 269 8301 3 753 740 0 1 0"
      ;;
    8)
      test-tcas  "172 1 289 259 961 5123 1 339 511 0 1 0"
      ;;
    9)
      test-tcas  "201 1 100 406 543 168 0 931 601 1 1 0"
      ;;
    10)
      test-tcas  "368 0 802 197 735 4310 0 190 127 1 0 0"
      ;;
    11)
      test-tcas  "649 1 130 559 818 7522 0 954 557 1 1 0"
      ;;
    12)
      test-tcas  "146 0 498 793 268 5854 1 222 94 1 1 1"
      ;;
    13)
      test-tcas  "629 1 190 439 133 3916 1 110 545 1 0 1"
      ;;
    14)
      test-tcas  "243 0 149 266 367 3794 3 923 190 0 0 0"
      ;;
    15)
      test-tcas  "709 0 186 762 703 8801 0 930 988 1 1 1"
      ;;
    16)
      test-tcas  "514 0 216 284 237 7604 3 365 402 0 1 1"
      ;;
    17)
      test-tcas  "2 0 564 252 692 1136 0 544 356 0 1 0"
      ;;
    18)
      test-tcas  "873 1 614 472 351 1221 3 857 262 1 1 0"
      ;;
esac
