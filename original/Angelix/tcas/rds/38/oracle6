#!/bin/bash
set -euo pipefail
SCRIPT_DIR=$(cd "$(dirname "${BASH_SOURCE[0]}")" && pwd)
test-tcas () {
    diff -q <(${ANGELIX_RUN:-eval} ./tcas $1) <($SCRIPT_DIR/../golden/tcas $1) > /dev/null
}

case "$1" in
    1)
      test-tcas  "40 0 99 119 324 9242 1 33 518 1 0 0"
      ;;
    2)
      test-tcas  "428 1 586 244 375 2590 1 292 544 1 0 0"
      ;;
    3)
      test-tcas  "777 0 217 547 9 1891 2 103 761 0 1 1"
      ;;
    4)
      test-tcas  "493 0 694 116 437 7868 2 536 466 0 1 0"
      ;;
    5)
      test-tcas  "918 0 559 360 825 8842 2 524 893 1 0 0"
      ;;
    6)
      test-tcas  "702 1 265 461 6 8637 2 568 761 1 0 0"
      ;;
    7)
      test-tcas  "441 1 923 82 500 8048 1 676 391 1 0 0"
      ;;
    8)
      test-tcas  "348 0 359 883 298 3007 1 292 600 0 1 0"
      ;;
    9)
      test-tcas  "48 0 505 116 465 8484 2 742 508 0 1 1"
      ;;
    10)
      test-tcas  "768 0 811 320 89 8955 0 518 89 0 0 1"
      ;;
    11)
      test-tcas  "222 1 402 83 377 4010 0 657 923 1 0 0"
      ;;
    12)
      test-tcas  "315 1 90 737 856 1369 1 453 990 0 1 0"
      ;;
    13)
      test-tcas  "540 0 45 5 626 9179 0 876 632 1 1 1"
      ;;
    14)
      test-tcas  "447 1 32 716 427 5020 3 242 922 0 0 1"
      ;;
    15)
      test-tcas  "996 1 490 6 956 566 0 978 616 0 1 0"
      ;;
    16)
      test-tcas  "795 1 501 753 412 8247 3 717 685 1 0 0"
      ;;
    17)
      test-tcas  "355 0 786 455 570 9646 3 417 42 1 0 1"
      ;;
esac
