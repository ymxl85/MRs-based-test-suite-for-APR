#!/bin/bash
set -euo pipefail
SCRIPT_DIR=$(cd "$(dirname "${BASH_SOURCE[0]}")" && pwd)
test-tcas () {
    diff -q <(${ANGELIX_RUN:-eval} ./tcas $1) <($SCRIPT_DIR/../golden/tcas $1) > /dev/null
}

case "$1" in
    1)
      test-tcas  "200 1 778 295 672 7460 1 545 945 0 1 0"
      ;;
    2)
      test-tcas  "477 1 687 653 879 356 1 629 417 1 0 1"
      ;;
    3)
      test-tcas  "861 1 715 923 137 700 0 587 783 1 1 1"
      ;;
    4)
      test-tcas  "767 0 606 866 862 868 0 574 354 0 0 1"
      ;;
    5)
      test-tcas  "591 1 926 530 500 108 1 62 1 0 0 1"
      ;;
    6)
      test-tcas  "954 1 332 488 86 992 3 316 167 1 1 1"
      ;;
    7)
      test-tcas  "547 0 805 811 60 4278 2 686 213 0 0 1"
      ;;
    8)
      test-tcas  "511 1 654 610 780 8916 3 219 312 0 0 1"
      ;;
    9)
      test-tcas  "742 0 471 859 605 319 1 809 642 1 1 1"
      ;;
    10)
      test-tcas  "47 1 115 429 25 4915 0 55 208 0 0 1"
      ;;
    11)
      test-tcas  "724 0 340 833 959 2250 1 374 349 0 0 1"
      ;;
    12)
      test-tcas  "851 1 809 67 115 7660 2 592 734 1 1 0"
      ;;
    13)
      test-tcas  "389 1 937 225 139 8694 2 273 583 1 1 0"
      ;;
    14)
      test-tcas  "148 1 306 110 262 7067 0 405 783 0 0 0"
      ;;
    15)
      test-tcas  "619 1 313 43 149 4971 3 819 670 0 0 1"
      ;;
    16)
      test-tcas  "847 1 195 328 817 47 3 734 403 1 1 0"
      ;;
    17)
      test-tcas  "648 0 653 478 723 7228 3 269 106 1 1 1"
      ;;
esac
