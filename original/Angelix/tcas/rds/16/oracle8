#!/bin/bash
set -euo pipefail
SCRIPT_DIR=$(cd "$(dirname "${BASH_SOURCE[0]}")" && pwd)
test-tcas () {
    diff -q <(${ANGELIX_RUN:-eval} ./tcas $1) <($SCRIPT_DIR/../golden/tcas $1) > /dev/null
}

case "$1" in
    1)
      test-tcas  "820 1 172 262 37 6702 0 635 400 1 0 0"
      ;;
    2)
      test-tcas  "195 1 943 662 147 6724 3 920 337 0 1 1"
      ;;
    3)
      test-tcas  "169 0 617 264 395 7596 0 288 863 0 1 0"
      ;;
    4)
      test-tcas  "693 0 169 171 812 8373 0 640 343 1 0 0"
      ;;
    5)
      test-tcas  "172 0 958 205 124 5081 0 271 575 1 0 0"
      ;;
    6)
      test-tcas  "986 1 545 263 835 2195 0 824 594 0 1 0"
      ;;
    7)
      test-tcas  "0 0 941 611 577 3146 3 504 524 1 1 0"
      ;;
    8)
      test-tcas  "386 1 517 826 600 1222 1 573 924 0 1 0"
      ;;
    9)
      test-tcas  "940 1 221 83 357 5966 0 172 534 1 0 1"
      ;;
    10)
      test-tcas  "770 1 456 540 175 9725 3 605 571 0 1 0"
      ;;
    11)
      test-tcas  "785 1 29 751 808 4632 1 408 830 1 1 1"
      ;;
    12)
      test-tcas  "811 1 997 288 348 1736 0 853 813 0 1 0"
      ;;
    13)
      test-tcas  "394 0 980 113 466 1473 3 869 916 0 0 1"
      ;;
    14)
      test-tcas  "411 1 626 683 368 8004 2 885 305 0 1 1"
      ;;
    15)
      test-tcas  "574 0 310 302 979 9284 2 642 649 0 0 1"
      ;;
    16)
      test-tcas  "622 1 979 894 993 9944 1 703 164 1 1 1"
      ;;
esac
