#!/bin/bash
set -euo pipefail
SCRIPT_DIR=$(cd "$(dirname "${BASH_SOURCE[0]}")" && pwd)
test-tcas () {
    diff -q <(${ANGELIX_RUN:-eval} ./tcas $1) <($SCRIPT_DIR/../golden/tcas $1) > /dev/null
}

case "$1" in
    1)
      test-tcas  "251 1 958 791 55 5287 3 288 809 1 0 0"
      ;;
    2)
      test-tcas  "529 1 789 621 471 988 0 168 418 0 0 0"
      ;;
    3)
      test-tcas  "675 1 803 853 147 1438 2 327 238 0 0 0"
      ;;
    4)
      test-tcas  "273 1 642 204 574 399 1 728 355 0 0 0"
      ;;
    5)
      test-tcas  "80 1 232 42 11 800 3 397 274 0 1 1"
      ;;
    6)
      test-tcas  "834 0 847 473 490 8413 3 774 125 1 1 0"
      ;;
    7)
      test-tcas  "191 1 333 233 630 3298 1 865 235 1 0 1"
      ;;
    8)
      test-tcas  "718 0 454 323 643 5182 2 146 693 1 1 1"
      ;;
    9)
      test-tcas  "692 1 804 691 187 7682 0 834 400 0 1 1"
      ;;
    10)
      test-tcas  "817 0 224 418 722 8510 3 579 553 0 1 1"
      ;;
    11)
      test-tcas  "10 0 334 983 492 8409 2 11 579 0 1 1"
      ;;
    12)
      test-tcas  "961 1 488 18 413 633 3 709 172 0 1 1"
      ;;
    13)
      test-tcas  "356 0 318 720 834 5336 0 196 339 0 1 0"
      ;;
    14)
      test-tcas  "447 1 891 678 463 9120 2 424 190 0 0 0"
      ;;
    15)
      test-tcas  "999 0 584 272 624 9444 3 902 641 1 0 1"
      ;;
    16)
      test-tcas  "619 1 819 77 547 6363 0 190 227 1 0 0"
      ;;
esac
