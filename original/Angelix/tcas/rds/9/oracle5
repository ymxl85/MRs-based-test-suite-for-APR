#!/bin/bash
set -euo pipefail
SCRIPT_DIR=$(cd "$(dirname "${BASH_SOURCE[0]}")" && pwd)
test-tcas () {
    diff -q <(${ANGELIX_RUN:-eval} ./tcas $1) <($SCRIPT_DIR/../golden/tcas $1) > /dev/null
}

case "$1" in
    1)
      test-tcas  "55 1 938 965 251 752 3 18 556 0 1 1"
      ;;
    2)
      test-tcas  "897 0 636 318 546 3948 1 756 474 0 0 0"
      ;;
    3)
      test-tcas  "665 0 55 123 434 2593 1 2 983 0 0 1"
      ;;
    4)
      test-tcas  "522 1 449 743 303 8704 0 569 795 0 0 1"
      ;;
    5)
      test-tcas  "83 1 832 463 273 1572 1 708 314 0 0 0"
      ;;
    6)
      test-tcas  "207 1 156 558 984 920 2 489 205 1 0 1"
      ;;
    7)
      test-tcas  "986 1 23 397 700 8448 0 976 590 1 1 0"
      ;;
    8)
      test-tcas  "505 1 544 350 821 49 0 290 438 1 1 0"
      ;;
    9)
      test-tcas  "392 0 802 968 259 4904 0 38 13 0 1 1"
      ;;
    10)
      test-tcas  "399 0 378 542 232 6181 2 862 231 1 1 1"
      ;;
    11)
      test-tcas  "132 0 622 775 867 1364 1 162 531 1 1 0"
      ;;
    12)
      test-tcas  "713 1 846 888 519 8125 1 275 275 1 0 0"
      ;;
    13)
      test-tcas  "471 0 253 60 965 4327 2 633 880 1 0 0"
      ;;
    14)
      test-tcas  "867 0 352 590 635 8021 2 581 737 0 1 0"
      ;;
    15)
      test-tcas  "599 1 670 717 696 5341 0 708 606 1 0 0"
      ;;
    16)
      test-tcas  "369 0 978 452 243 850 1 654 457 1 0 0"
      ;;
    17)
      test-tcas  "18 1 719 251 974 6293 0 462 789 0 0 1"
      ;;
    18)
      test-tcas  "647 1 163 722 568 9776 1 579 206 1 0 0"
      ;;
    19)
      test-tcas  "49 1 761 975 563 8134 3 670 489 1 1 1"
      ;;
    20)
      test-tcas  "108 0 406 518 801 686 2 375 624 1 1 0"
      ;;
esac
