#!/bin/bash
set -euo pipefail
SCRIPT_DIR=$(cd "$(dirname "${BASH_SOURCE[0]}")" && pwd)
test-tcas () {
    diff -q <(${ANGELIX_RUN:-eval} ./tcas $1) <($SCRIPT_DIR/../golden/tcas $1) > /dev/null
}

case "$1" in
    1)
      test-tcas  "711 1 957 138 388 2629 1 545 788 0 0 0"
      ;;
    2)
      test-tcas  "384 0 439 359 691 7817 2 33 155 1 0 1"
      ;;
    3)
      test-tcas  "457 1 549 828 982 6749 2 46 518 1 0 1"
      ;;
    4)
      test-tcas  "393 0 351 598 165 2423 1 303 646 0 1 0"
      ;;
    5)
      test-tcas  "544 0 403 390 241 7588 0 863 241 1 0 0"
      ;;
    6)
      test-tcas  "681 0 968 905 830 9119 1 701 949 0 1 0"
      ;;
    7)
      test-tcas  "968 1 384 958 438 5565 0 400 896 1 0 0"
      ;;
    8)
      test-tcas  "292 0 8 230 429 9057 3 245 387 1 0 1"
      ;;
    9)
      test-tcas  "799 1 886 426 428 9071 1 500 933 0 1 0"
      ;;
    10)
      test-tcas  "878 1 945 199 25 2836 3 813 693 1 0 1"
      ;;
    11)
      test-tcas  "496 0 938 407 95 5379 3 371 689 1 1 1"
      ;;
    12)
      test-tcas  "839 0 661 18 894 1365 0 742 708 1 1 1"
      ;;
    13)
      test-tcas  "562 0 132 363 215 948 3 245 784 0 1 0"
      ;;
    14)
      test-tcas  "709 1 386 393 411 2915 0 182 390 1 0 1"
      ;;
    15)
      test-tcas  "385 0 362 149 937 323 0 349 152 1 0 1"
      ;;
    16)
      test-tcas  "715 0 66 810 232 6779 1 919 806 0 1 0"
      ;;
    17)
      test-tcas  "322 0 660 429 322 5293 1 78 493 0 0 1"
      ;;
esac
