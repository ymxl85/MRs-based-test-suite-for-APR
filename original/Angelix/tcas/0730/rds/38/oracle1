#!/bin/bash
set -euo pipefail
SCRIPT_DIR=$(cd "$(dirname "${BASH_SOURCE[0]}")" && pwd)
test-tcas () {
    diff -q <(${ANGELIX_RUN:-eval} ./tcas $1) <($SCRIPT_DIR/../golden/tcas $1) > /dev/null
}

case "$1" in
    1)
      test-tcas  "881 1 945 584 928 3719 1 229 531 1 1 1"
      ;;
    2)
      test-tcas  "732 0 598 374 539 178 0 581 186 0 1 0"
      ;;
    3)
      test-tcas  "311 0 372 374 510 4867 1 78 57 0 0 1"
      ;;
    4)
      test-tcas  "107 0 676 599 372 8692 0 141 77 1 1 0"
      ;;
    5)
      test-tcas  "978 1 902 206 386 5446 3 973 405 0 1 1"
      ;;
    6)
      test-tcas  "68 1 162 888 637 2735 1 151 125 0 1 1"
      ;;
    7)
      test-tcas  "982 1 87 950 0 5593 3 769 688 1 0 1"
      ;;
    8)
      test-tcas  "781 0 397 333 80 6018 0 362 523 1 1 1"
      ;;
    9)
      test-tcas  "237 1 776 641 343 2781 2 544 463 1 0 1"
      ;;
    10)
      test-tcas  "162 0 387 754 101 9222 2 703 263 0 0 1"
      ;;
    11)
      test-tcas  "429 0 167 805 41 2225 1 207 674 1 0 1"
      ;;
    12)
      test-tcas  "605 1 756 257 418 3305 2 462 104 0 1 1"
      ;;
    13)
      test-tcas  "899 1 658 478 952 2609 0 683 990 1 0 1"
      ;;
    14)
      test-tcas  "317 0 907 584 869 8724 2 568 538 0 0 0"
      ;;
    15)
      test-tcas  "656 0 146 418 360 1450 1 43 527 0 1 1"
      ;;
    16)
      test-tcas  "123 0 689 368 83 1152 0 975 313 1 1 1"
      ;;
    17)
      test-tcas  "989 0 683 237 108 6946 1 204 154 1 1 1"
      ;;
esac
