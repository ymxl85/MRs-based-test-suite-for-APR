#!/bin/bash
set -euo pipefail
SCRIPT_DIR=$(cd "$(dirname "${BASH_SOURCE[0]}")" && pwd)
test-replace () {
    diff <(${ANGELIX_RUN:-angelix-eval} ./replace "$1" "$2"  "$3") <(angelix-eval $SCRIPT_DIR/../golden/replace "$1" "$2" "$3")
}

case "$1" in
    1)
      test-replace  "@O" "" "/D 85,]%dVrFXG&>"
      ;;
    2)
      test-replace  ":!" "ttg3" "5\\M}cX2]Y=k"
      ;;
    3)
      test-replace  "mr" "" "6D\"e\"}70&nG|vYhiPNo"
      ;;
    4)
      test-replace  "9gl6" "2" "Aw\`cu1x;}}W2B&sh"
      ;;
    5)
      test-replace  "j\$WC" "j\$" "U,&yR&l6})}wb*7iI"
      ;;
    6)
      test-replace  "}?0" "A]K+" "gz*}GE93&+HwNl--487n"
      ;;
    7)
      test-replace  "" "p E" "Jp)dP/\"\"P_nbTh?S"
      ;;
    8)
      test-replace  "xP3" "A6i" "L_9N@"
      ;;
    9)
      test-replace  "" "" "q,/;V"
      ;;
    10)
      test-replace  "[.%" "y" ":^5R0iSr;x=U"
      ;;
    11)
      test-replace  "~_~" "Z" "!"
      ;;
    12)
      test-replace  "" "iQ+" "mS JbJ=2j;Ex,<"
      ;;
    13)
      test-replace  "R8b" "Q&" "U:c&6\"}"
      ;;
    14)
      test-replace  "{UXC" "" "A?V\`6"
      ;;
    15)
      test-replace  "#U" "!V" "Q(bq >Ezj"
      ;;
    16)
      test-replace  "" "#" "gepQ*ldw&QT&"
      ;;
    17)
      test-replace  "z|@)" "" "m(\`;6\`ls"
      ;;
    18)
      test-replace  "S%" "8rf@" "91IyP5?|RRe/g\"^pW"
      ;;
    19)
      test-replace  "gS%X" "" "m"
      ;;
    20)
      test-replace  "hxq" "-t}" "nCh7b"
      ;;
    21)
      test-replace  "" "" "p28O].H&w;}I.wLI%"
      ;;
    22)
      test-replace  "1<" "[\"" "k&ybKwZh=h1K3C"
      ;;
    23)
      test-replace  ";q+h" "]0y]" "ZoAPx6YHvQaS*8U]"
      ;;
    24)
      test-replace  "G^" "p" "EfGVI"
      ;;
    25)
      test-replace  "Lq" "" "|&!K1O{1s"
      ;;
    26)
      test-replace  "1" "" ">Do6x|Sl;guu@El)u4* "
      ;;
    27)
      test-replace  ")P)>" "" "/. j"
      ;;
    28)
      test-replace  "ZtJL" "6" "aCF^QglC,"
      ;;
    29)
      test-replace  "|RmK" "<~}9" "9^"
      ;;
    30)
      test-replace  "C=" "" "tt/CQN\$0q^Ulj^nrl"
      ;;
    31)
      test-replace  "i" "G*|" "#8;yB60;T2H@eXt^"
      ;;
    32)
      test-replace  "" "" "FbcN}?;PR"
      ;;
    33)
      test-replace  "H" "qyp" "lmv5Az#)l"
      ;;
    34)
      test-replace  "V" "&jG" "sq"
      ;;
    35)
      test-replace  "u;Ic" "0" "rZ:<}!voZ_hETE 3"
      ;;
    36)
      test-replace  "Pxy\`" "3[TH" "es*,"
      ;;
    37)
      test-replace  "A" "q[" "%\$x3WIWD-t"
      ;;
    38)
      test-replace  "2F[" ")tQ" "[..RnxC"
      ;;
    39)
      test-replace  "" "P#g" "}-"
      ;;
    40)
      test-replace  ".*U" "mgdl" "OYj "
      ;;
    41)
      test-replace  "6" "[hlo" "SivZpy"
      ;;
    42)
      test-replace  "?*" "g" "2Rek{(Q8E8Hr"
      ;;
    43)
      test-replace  "Qev" "~4<" "tY"
      ;;
    44)
      test-replace  "&" "Sv" "P\$xMog&mS"
      ;;
    45)
      test-replace  "" "0O" "2Z("
      ;;
    46)
      test-replace  "" "\$?" "4~\$okj{C>+F}07N[e"
      ;;
    47)
      test-replace  "v" "m\`:" "=&."
      ;;
    48)
      test-replace  "" "Lze_" ";;D"
      ;;
    49)
      test-replace  "" "2" "R7\`Yxe?\`]e9q"
      ;;
    50)
      test-replace  "N" "" "3(I"
      ;;
    51)
      test-replace  "" "\`hB" "!N*u\$fUiGbr8Y"
      ;;
    52)
      test-replace  "" "" "RuUI.1J;2."
      ;;
    53)
      test-replace  "iq" "C" "\"(CHUNmNw .cLW"
      ;;
    54)
      test-replace  "3vsH" "" "IenRa1%@RLl"
      ;;
    55)
      test-replace  "s" "Q" "6^k3f<<.;fw\"3+ua:wz"
      ;;
    56)
      test-replace  "*Sh" "AR" "JlW~=\"..W#"
      ;;
    57)
      test-replace  "+n];" "" "hH{O#\\3iUYUxM5"
      ;;
    58)
      test-replace  "v,~A" "e.j(" "0o/A/)1"
      ;;
    59)
      test-replace  "wA?" "" "@Z"
      ;;
    60)
      test-replace  "E^L" "jEe" "a=uT3?yPA/kO[v,8Y"
      ;;
    61)
      test-replace  "pJ-#" "-r<" "GKTX=y"
      ;;
    62)
      test-replace  "Xjl" "8/" "8Gi:zR9<"
      ;;
    63)
      test-replace  "bL.*" "g" "\"6k0v\\sJC"
      ;;
    64)
      test-replace  "TS" "x8\$" "Z2fUk,KWqNjwe"
      ;;
    65)
      test-replace  ")" "S7IZ" "NmFPxK}"
      ;;
    66)
      test-replace  "Q" "vy" "\""
      ;;
    67)
      test-replace  ",s8)" "A" ":lU\\nEca3bu(%N_\$D-"
      ;;
    68)
      test-replace  "B" "" "/V?s!@"
      ;;
    69)
      test-replace  "rHD" "Fm" "+\"<#ZS)G\$_|3BDw/_d5j"
      ;;
    70)
      test-replace  "" "\\k}-" "css}mXRz?&4Xz DK[~Y"
      ;;
    71)
      test-replace  "\\" "0:i" "1xX%JHg?>1"
      ;;
    72)
      test-replace  "iW" "kbX" "R}Q\"bT^qL*ZwB;=8-"
      ;;
    73)
      test-replace  "" "N\`3" "!bdI7XT,<rzF"
      ;;
    74)
      test-replace  "7&r|" "" ";i?i#q@}K\\K Qz\"f"
      ;;
    75)
      test-replace  "&Wy" "" "&anBmI<f"
      ;;
    76)
      test-replace  "Mb" "+bLS" "nh+h/g5Qx"
      ;;
    77)
      test-replace  "5" "e^" "A.Q;py8E"
      ;;
    78)
      test-replace  "7[" "{(ou" "Smp|-IJXcD#dxS\"i9[TB"
      ;;
esac
