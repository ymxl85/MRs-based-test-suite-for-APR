#!/bin/bash
set -euo pipefail
SCRIPT_DIR=$(cd "$(dirname "${BASH_SOURCE[0]}")" && pwd)
test-replace () {
    diff <(${ANGELIX_RUN:-angelix-eval} ./replace "$1" "$2"  "$3") <(angelix-eval $SCRIPT_DIR/../golden/replace "$1" "$2" "$3")
}

case "$1" in
    1)
      test-replace  "U!" "n2l{" "A\$mpPItdEH5SLt"
      ;;
    2)
      test-replace  "XoGK" "" "]DLcT*S@e\"_"
      ;;
    3)
      test-replace  "4\`" "D\"2" "..C%f!nK1!{C]S"
      ;;
    4)
      test-replace  "_" "J" "!-c_H\\&t}C,@t3R"
      ;;
    5)
      test-replace  "Wc" "ThXN" "@j6"
      ;;
    6)
      test-replace  "Hr a" "u" "HDJ{FpX<])f"
      ;;
    7)
      test-replace  "K" "2" ";[|B+!by"
      ;;
    8)
      test-replace  "B3=" "" "@wc!Enbn[.*"
      ;;
    9)
      test-replace  "&I" "@\`\\1" "#kW?;(g6\":l_xvo"
      ;;
    10)
      test-replace  "" "R" "%Wz:X"
      ;;
    11)
      test-replace  "a\"" "2E#)" "VQ"
      ;;
    12)
      test-replace  "4N:" "" "**)?:A=[Z2DGJI@{gO"
      ;;
    13)
      test-replace  "Mf" "=" "Hk_rl_g{l5p.A^^;.Ra"
      ;;
    14)
      test-replace  "u{" "\`JiQ" "b=eNYc*"
      ;;
    15)
      test-replace  "98" "1" "Xm"
      ;;
    16)
      test-replace  "" "mbD|" "QHL"
      ;;
    17)
      test-replace  "9b,{" "/iY+" "Wt9vVd\"SCRDdl[^K!?{"
      ;;
    18)
      test-replace  "" "@KI" "*8umPc4@@_."
      ;;
    19)
      test-replace  "md" "21" "p}uu%bj OWf#]h2mV"
      ;;
    20)
      test-replace  ";V" "t6QS" "4vXf0F/H]WZziq"
      ;;
    21)
      test-replace  "" "tj" "k"
      ;;
    22)
      test-replace  "2" "" "Oh@eRB1y;"
      ;;
    23)
      test-replace  "zizz" "!zj(" "B@0dK"
      ;;
    24)
      test-replace  "4Koi" "vO\\" "A"
      ;;
    25)
      test-replace  "a9l" "Y7" ">"
      ;;
    26)
      test-replace  "XCt" "4" "U4!X;BS^&a[dd"
      ;;
    27)
      test-replace  "Q" "uA" "4"
      ;;
    28)
      test-replace  "9?0" "" "Z\$&,cr}"
      ;;
    29)
      test-replace  "gB;" "!" "Zivqp.c#I=m"
      ;;
    30)
      test-replace  "e7#" " " ";W"
      ;;
    31)
      test-replace  ":" "^" "RL\`]1.!Yp&5;[Z"
      ;;
    32)
      test-replace  ":E" "=" "mM}GmUfI*j1"
      ;;
    33)
      test-replace  "Q24" "tA@." "\\XQ:{Q6/0HlUEI\$<"
      ;;
    34)
      test-replace  "W" ";tGX" "8:4:8z4xY4fs!Xrz"
      ;;
    35)
      test-replace  "" "2" "\$"
      ;;
    36)
      test-replace  "h" "#" "8\\dy*T]bl].#r"
      ;;
    37)
      test-replace  ">" "?me|" "[."
      ;;
    38)
      test-replace  "" "" "I"
      ;;
    39)
      test-replace  "n#" "l" " 4,y"
      ;;
    40)
      test-replace  "+X=A" " RD5" "&\$"
      ;;
    41)
      test-replace  "" "U,K" "||"
      ;;
    42)
      test-replace  "u-" "e0)l" "uDdwY<ol~g)Ks#r"
      ;;
    43)
      test-replace  "isQA" "<P\\" "dOBn"
      ;;
    44)
      test-replace  "830" "\$" "+#/Byj+{*(Rim)^5f"
      ;;
    45)
      test-replace  "oj#" "^" "F9 \$4U"
      ;;
    46)
      test-replace  "}" "" "WT{)%67?P?!vE(y|Ov"
      ;;
    47)
      test-replace  "+GO-" "" "5oAM]"
      ;;
    48)
      test-replace  "6y" "wp" "0yq\$3z?!%Vpj xV1~w\\"
      ;;
    49)
      test-replace  "" "G\$u" "QAkcT.\\k74"
      ;;
    50)
      test-replace  "" "|\"" "zIL~,\\"
      ;;
    51)
      test-replace  "6x)[" "[N#" "v*;F<z6B,Bp6xh7Qdy"
      ;;
    52)
      test-replace  "~WC" "^Z" " N}bhA"
      ;;
    53)
      test-replace  "" "n" "\$\"j10-71&"
      ;;
    54)
      test-replace  "9)(" "A=Q" "{VBaQj=}|&,%k7=2."
      ;;
    55)
      test-replace  "QZ}" "" "6X#Zl41AFeHck\""
      ;;
    56)
      test-replace  "v~al" "1*x>" "\`\`n!MIRJfA0&"
      ;;
    57)
      test-replace  "\`{" "#:^l" "?*KQ~L73"
      ;;
    58)
      test-replace  "" "" "hj1jq(Ws)!yXir"
      ;;
    59)
      test-replace  ",Vk" "4_g" "T0F(Q/3+B[# i\`fT(!3"
      ;;
    60)
      test-replace  "0" "?~" "\\2sYZJ)/nw"
      ;;
    61)
      test-replace  "uM" "w^" "D+Di%d[_>g^jOQ\`%Y>+."
      ;;
    62)
      test-replace  "w~\$" "Z" "*Qo/q\\t  d0!yu(<r0"
      ;;
    63)
      test-replace  "+<<" "Y>)i" "9QjB"
      ;;
    64)
      test-replace  "9v~" "k[" "eQ"
      ;;
    65)
      test-replace  "n9" "wN\$" "%6_Q\`|S&Yu}h((}o/)31"
      ;;
    66)
      test-replace  "" "SCx" "8o0r&64Ep9;S"
      ;;
    67)
      test-replace  "2{Y" "RN " ",Qq7J@7>q*\\H"
      ;;
    68)
      test-replace  "(O/" "{^!7" "kFy)7@bLP|Rvm%"
      ;;
    69)
      test-replace  "zh)" "j6Or" "0"
      ;;
    70)
      test-replace  "" "V3k[" "P.3e!X,l5@[W"
      ;;
    71)
      test-replace  "D" "B" "G\`HRkCj/&MXs"
      ;;
    72)
      test-replace  ">" "" "dSbKl)(O#QvN"
      ;;
    73)
      test-replace  "P8Vc" "\"G)" "=~.-Ko.6F+~!7;\$C"
      ;;
    74)
      test-replace  "" "i&" "gkHL/%*6GX@h+oL?z9"
      ;;
    75)
      test-replace  "-02L" "KY" "PP7^J\`IDUi&mY"
      ;;
    76)
      test-replace  "J" "FG+u" "\`9;E=6Tr%/7)<="
      ;;
    77)
      test-replace  "+)>" "X" "8"
      ;;
    78)
      test-replace  "K/qV" "@" "VNm\\PI/usyKkbL!"
      ;;
    79)
      test-replace  "\\U" "!;" "kMHJ[NQ;nALU,>T"
      ;;
    80)
      test-replace  "" "C" "W<~b1ez{|dL?Mf;Z."
      ;;
    81)
      test-replace  "" "7Z2v" "Y !4+{rXbL.JXm]S"
      ;;
    82)
      test-replace  "Rb8!" "&" "T"
      ;;
    83)
      test-replace  "AXVc" "Su" "I?4HXY%w]V6z8s])9yP"
      ;;
    84)
      test-replace  "A@&4" "<e" "Es,b<[!TAvLmi d*3Kj0"
      ;;
    85)
      test-replace  "e" "d" "Pg/_74Ufn*1h8=Ob5E"
      ;;
    86)
      test-replace  "16bY" "29J" ":]dFNA"
      ;;
    87)
      test-replace  "" "s&" "7"
      ;;
    88)
      test-replace  "*/d" "i" "m<1p@,r:9H"
      ;;
    89)
      test-replace  "" "Y&X" "aRNK\""
      ;;
    90)
      test-replace  "" "@" "]m"
      ;;
    91)
      test-replace  ")jmW" "m" "VfAb]h#~\$V6qJzb&R"
      ;;
    92)
      test-replace  "B" "&AS" "FKk~^C95->"
      ;;
    93)
      test-replace  "\`o" "" "X[v)CSxCDcE]{0e"
      ;;
esac
